include('../axioms/key-value-store.ax').

tff(put_original_uniqueness,conjecture,
    ! [A: $array($int,list),B: $array($int,list),K: $int,V: $int] :
      ( ( put(A,K,V) = B )
     => ! [L: $int,M: $int] :
          ( ( ( get(A,L) != nil )
            & ( get(A,M) != nil )
            & $lesseq(K,L) )
         => ( K != L ) ) ) ).